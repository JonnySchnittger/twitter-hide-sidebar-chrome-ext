{
  "manifest_version": 2,
  "name": "Twitter Hide Sidbar Extension",
  "version": "0.1.0",
  "description": "Hides the sidebar on twitter.com",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ]
}